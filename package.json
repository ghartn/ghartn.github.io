{
	"name": "lentra",
	"version": "0.1.0",
	"private": true,
	"homepage": "http://lentra.ca",
	"dependencies": {
		"gh-pages": "^1.2.0",
		"globby": "^9.0.0",
		"react": "^16.7.0",
		"react-dom": "^16.7.0",
		"react-router-dom": "^4.3.1",
		"react-scripts": "2.1.3"
	},
	"scripts": {
		"analyze": "source-map-explorer build/static/js/main.*",
		"build-style": "postcss src/styles/index.css -o src/index.css",
		"watch-style": "postcss src/styles/index.css -o src/index.css -w",
		"eject": "react-scripts eject",
		"start": "npm-run-all -p watch-style start-app",
		"build": "npm-run-all build-style build-app",
		"start-app": "react-scripts start",
		"build-app": "react-scripts build",
		"test": "react-scripts test --env=jsdom",
		"predeploy": "npm run build",
		"deploy": "gh-pages -d build"
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"devDependencies": {
		"autoprefixer": "^9.4.5",
		"npm-run-all": "^4.1.5",
		"postcss-cli": "^6.1.1",
		"tailwindcss": "^0.7.3"
	}
}
